<Window x:Class="WPFStylesPart3ResourceDictionaries.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFStylesPart3ResourceDictionaries"
        mc:Ignorable="d"
        Title="Styles Part 3 Resource Dictionaries" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="50">
            <Label Style="{StaticResource BigTitleLabel}">BIG TITLE</Label>
            <Label Style="{StaticResource TextBoxLabel}">Full Name</Label>
            <TextBox FontSize="18"/>
            <Button Style="{StaticResource SubmitButton}">Submit</Button>
            <Button Style="{StaticResource CancelButton}">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
